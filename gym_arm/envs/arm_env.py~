from gym.utils import seeding
import numpy as np

class ArmEnv(gym.Env):
  metadata = {
    'render.modes': ['human', 'rgb_array'],
    'video.frames_per_second': 30
  }

  def __init__(self):
    print 'init'

  def _step(self, action):
    print 'step'

  def _reset(self):
    print 'reset'

  def _render(self, mode='human', close=False):
    print 'render'

